{
  "name": "jodc_website",
  "description": "Main website for JODC",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "webp": "cd src && node webp.js",
    "fmt": "prettier --write --plugin-search-dir=. './**/*.{svelte,js,html,css}'",
    "fmt:check": "prettier --check --plugin-search-dir=. './**/*.{svelte,js,html,css}'",
    "lint:check": "npx eslint src/"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^2.16.1",
    "gray-matter": "^4.0.2",
    "lazysizes": "^5.3.0",
    "marked": "^1.2.7",
    "polka": "next",
    "prismjs": "^1.23.0",
    "reading-time": "^1.2.1",
    "sirv": "^1.0.10",
    "svelte-awesome": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "eslint": "^7.18.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.36.2",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.10",
    "svelte": "^3.31.2"
  }
}
